fun relist(s::sx:(int)list,k:(int)list)=
if null(sx)=true then s::k
else relist(sx,s::k);

fun rerlist(s:(int)list,k:(int)list)=
let val p:(int)list=
if null(s)=true then nil
else relist(s,k)
in p
end;
val s1:(int)list=rerlist(s,nil);
