val n:int=getInt();
val t:(IntInf.int)list=getIntInfTable(n);
val c:IntInf.int=getIntInf();
fun find_change(s::sx:(IntInf.int)list,c:IntInf.int,sum:IntInf.int):IntInf.int=
if null(sx)=true then sum+c div s
else if c=0 then sum
else find_change(sx,c mod s,sum+(c div s));
val k:IntInf.int=0;
printIntInf(find_change(t,c,k));
